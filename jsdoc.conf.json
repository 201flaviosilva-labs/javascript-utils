{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "systemName": "@flaviosilva/utilsjs",
    "systemSummary": "Just simple utils for javascript :)",
    "dateFormate": "D/M/YYYY",
    "footer": "Source code: https://gitlab.com/201flaviosilva/utilsjs",
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "encoding": "utf8",
    "package": "package.json",
    "recurse": true,
    "destination": "docs",
    "readme": "README.md",
    "template": "./node_modules/foodoc/template"
  }
}
