{
  "name": "201flaviosilva-utils",
  "version": "1.2.6",
  "description": "Just simple utils for javascript :)",
  "main": "src/index.js",
  "homepage": "https://201flaviosilva.gitlab.io/utilsjs/",
  "author": "201flaviosilva",
  "license": "MIT",
  "files": [
    "build/",
    "src/",
    "types/",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "clear": "rm -rf types && rm -rf docs && rm -rf build",
    "jsDoc": "jsdoc -c jsdoc.conf.json",
    "types": "npx -p typescript tsc src/*.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "compile": "webpack --mode production",
    "compile:css": "css-minify -d src/CSS/ -o build",
    "build": "npm run clear && npm run compile && npm run compile:css && npm run jsDoc && npm run types",
    "bump": "npm run build && np  --no-tests"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-syntax-import-assertions": "^7.18.6",
    "babel-loader": "^8.2.5",
    "clean-jsdoc-theme": "^4.0.7",
    "css-minify": "^2.0.0",
    "jsdoc": "^3.6.10",
    "np": "^7.6.1",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "keywords": [
    "utils",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/201flaviosilva/201flaviosilva-utils.js.git"
  },
  "bugs": {
    "url": "https://gitlab.com/201flaviosilva/201flaviosilva-utils.js/issues"
  }
}
