{
  "name": "201flaviosilva-utils",
  "version": "1.2.10",
  "description": "Just simple utils for javascript :)",
  "main": "src/index.js",
  "homepage": "https://201flaviosilva-labs.github.io/javascript-utils/",
  "author": "201flaviosilva",
  "license": "MIT",
  "files": [
    "lib/",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "download-CSSFreeStyle": "npm i --prefix lib/CSSFreeStyle",
    "download-dom": "npm i --prefix lib/DOM",
    "download-vanilla": "npm i --prefix lib/Vanilla",
    "download": "npm-run-all --parallel download-CSSFreeStyle download-dom download-vanilla",
    "jsdoc-dom": "npm run jsdoc --prefix lib/DOM",
    "jsdoc-vanilla": "npm run jsdoc --prefix lib/Vanilla",
    "jsdoc": "npm-run-all --parallel jsdoc-dom jsdoc-vanilla",
    "build-CSSFreeStyle": "npm run build --prefix lib/CSSFreeStyle",
    "build-dom": "npm run build --prefix lib/DOM",
    "build-vanilla": "npm run build --prefix lib/Vanilla",
    "build": "npm-run-all --parallel build-CSSFreeStyle build-dom build-vanilla",
    "bump-CSSFreeStyle": "npm run bump --prefix lib/CSSFreeStyle",
    "bump-dom": "npm run bump --prefix lib/DOM",
    "bump-vanilla": "npm run bump --prefix lib/Vanilla",
    "bump": "npm-run-all --parallel bump-CSSFreeStyle bump-dom bump-vanilla"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/201flaviosilva-labs/javascript-utils.git"
  },
  "bugs": {
    "url": "https://github.com/201flaviosilva-labs/javascript-utils/issues"
  }
}
